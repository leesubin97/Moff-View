	<script>
		function goSearch(){
			var category_code2 = $("#category_code2").val();
			if($("#category_code2").val()==""){
				alert("공간을 선택하세요.");
				return;
			}

			$("#category_code1").val(category_code2.substring(0,2));

			var form = document.getElementById("searchForm");
			form.action = "/zws/product/getList.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB";
			form.submit();
		}
		
		function goPrev(pid) {
            var form = $('#linkForm')[0];
            if (form.cmd.value == 'detail') {
                location.href = "/zws/product/detail.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB?pid=" + 206 + "&currentPage=1&itemSize=12&searchStartDate=&searchEndDate=&orderBy=&category_code1=&category_code2=&style=&space_code=-1&area_code=-1&price_code=-1&region1=&region2=";
            }
        }

        function goNext(pid) {
            var form = $('#linkForm')[0];
            if (form.cmd.value == 'detail') {
                location.href = "/zws/product/detail.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB?pid=" + 208 + "&currentPage=1&itemSize=12&searchStartDate=&searchEndDate=&orderBy=&category_code1=&category_code2=&style=&space_code=-1&area_code=-1&price_code=-1&region1=&region2=";
            }
        }

        function doZzim(_this,pid){

            var me = $(_this);
            var url = '/zws/zzim/create.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB';
            var flag = false;
            if (me.hasClass('active')){
                url = '/zws/zzim/delete.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB';
                me.removeClass('active');
            } else {
                flag = true;
                me.addClass('active');
            }

            $.ajax({
                url: url,
                type: "post",
                data: "pid=" + pid + "&type=1",
                dataType: "json",
                success : function (responseData) {
                    var data = responseData;
                    if(!data){
                        alert("통신 에러");
                    }
                    if(data.success){
                        var zzim_cnt = data.data.zzim_cnt;
                        var checkZzimPid = data.data.checkZzimPid;

                        if(!flag && checkZzimPid) {
                            me.removeClass('active');
                        }
                        
                        var html = '<i class="fa fa-paperclip">' + zzim_cnt + '</i>';

                        $("#product_zzim_cnt").html(html);
                    }
                },
                error: function(request,status,error){
                    if(request.status==417){
                        alert("이미 스크랩에 존재합니다.");
                    } else if(request.status==401){
                        var responseJSON = request.responseJSON;
                        var cause = responseJSON.cause;
                    } else {
                        alert("스크랩 중에 오류가 발생되었습니다. code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
                    }

                    me.removeClass('active');
                }
            });
        }

		/*
		무료견적신청 레이어를 오픈한다.
		 */
		function openSimpleEstimateLayer(){
			var form = document.getElementById("estimateForm");
            $("#pid").val(207);
            form.action = "/zws/estimate/create.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB";
            form.method = "get";
            form.submit();
		}
		
 		$(document).ready(function(){
			setTimeout(initializeRelatedProduct, 3000);
			if(navigator.userAgent.match(/android/i) || navigator.userAgent.match(/(iphone)|(ipod)|(ipad)/i)){
				$("#footer").hide().find(".footer_imgbanner").remove();	
			}
 		});

		function initializeRelatedProduct(){
			getData();
			$(window).scroll(function() {
				if($(window).scrollTop()+1 >= $(document).height() - $(window).height()){
					getData();
				}
			});
		}


        //2017-01-03 list 데이터 ajax요청
		var callAjaxFlag = true;
		function getData() {
			if(!callAjaxFlag) return false;

            var currentPage = $("#currentPage").val();

			var params = $("#linkForm").serialize();
            if(currentPage != 0 && NumberCheck(currentPage)) {
                $("#currentPage").val(++currentPage);
            }

            $.ajax({
                url: '/zws/product/getListAjax.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB',
                type: "post",
                data: params,
                cache:false,
                dataType: "json",
                beforeSend: function(){
                    $(".loading").show();
                },
                success : function (data) {
  
                    $(".loading").hide();
  
                    var successType = data.success;
  
                    if(successType) {
                        var resultData = data.data;
                        if(isNotEmpty(resultData)) {
                            var pagingObject = resultData.pagingObject;
                            var resultList = pagingObject.resultList;
                            var resultLength = resultList.length;
  
                            if(resultLength > 0) {
                                for(var i = 0; i < resultLength; i++) {

                                    var price = resultList[i].price;

                                    if(isNotEmpty(price)) {
                                     	price =  price + "만원";
									} else {
                                        price = "";
									}


                                	var data = {
                                        "file_path" : resultList[i].file_path,
                                        "thumbnail" : resultList[i].thumbnail,
                                        "hit_cnt" : kFormatter(resultList[i].hit_cnt),
                                        "like_cnt" : resultList[i].like_cnt,
                                        "zzim_cnt" : resultList[i].zzim_cnt,
                                        "share_cnt" : resultList[i].share_cnt,
                                        "pid" : resultList[i].pid,
                                        "subject" : resultList[i].subject,
                                        "tags" : resultList[i].tags,
                                        "space" : resultList[i].space,
                                        "category_name1" : resultList[i].category_name1,
                                        "category_name2" : resultList[i].category_name2,
                                        "price" : price,
                                    };
                                    drawTemplate(data);
                                }
                            } else {
                                callAjaxFlag = false;
                                $("#footer").show();
//                                $(".footer_imgbanner").hide();
                            }
                        }
                    }
                },
                error: function(request,status,error){
                    alert("시공사례를 로드하는중 오류가 발생되었습니다. code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
                }
            });

        }
      
        function drawTemplate(data) {
            //핸들바 템플릿 가져오기
            var source = $("#li-template").html();
            //핸들바 템플릿 컴파일
            var template = Handlebars.compile(source);
            var selector_add = $("#list_title");

            var temp_html = template(data).toString();
            selector_add.append(temp_html);


            $(".tag_price").each(function () {
                // console.dir($(this));
				var temp = $(this).text();

				if(isEmpty(temp)) {
                	$(this).remove();
            	}
            });
        }

		//페이스북, 카카오톡, 카카오스토리, 밴드, 라인, 트위터
		function shareSns(snsType, shareTitle, shareImageUrl, pid){
			//퍼가기 카운트 증가
            if(isNotEmpty(pid)) {
                shareCntUpdate(pid);
            }
			var cur_url = location.href.replace("localhost:8080","zipdoc.co.kr");
			var _url = encodeURIComponent(cur_url);
			var _txt = encodeURIComponent(shareTitle);
			var _br = encodeURIComponent("\n");
			//var _br  = encodeURIComponent('\r\n');
			//Kakao.init('5f524abf1bf3ba92ddddb404113271ad');
			var _targetUrl = "";

			if(snsType=="facebook"){
				_targetUrl = "http://www.facebook.com/sharer/sharer.php?u=" + _url;
				window.open(_targetUrl);
			}else if(snsType=="naverblog"){
				_targetUrl = "http://share.naver.com/web/shareView.nhn?url=" + _url + "&title=" + _txt;
				window.open(_targetUrl);
			}else if(snsType=="kakaotalk"){
				if(navigator.userAgent.match(/android/i) || navigator.userAgent.match(/(iphone)|(ipod)|(ipad)/i)){
					Kakao.Link.createTalkLinkButton({
						container: '#kakao-link-btn',
						label: shareTitle,
						image: {
							src: shareImageUrl,
							width: '300',
							height: '200'
						},
						webButton : {
							text: '시공사례로 이동',
							url: "product/detail.do?pid=" + pid
						}
					});
				}else{
					alert("카카오톡 공유하기 기능은 모바일에서만 사용할 수 있습니다.");
				}
			}else if(snsType=="kakaostory"){
				Kakao.Story.share({
					url: cur_url,
					text: '카카오스토리로 이동'
				});
			}else if(snsType == "band"){
				_targetUrl = "http://www.band.us/plugin/share?body="+ _txt + "&route=" + _url, "share_band", "width=410, height=540, resizable=no";
				window.open(_targetUrl);
			}else if(snsType == "Line"){
				if(navigator.userAgent.match(/android/i) || navigator.userAgent.match(/(iphone)|(ipod)|(ipad)/i)){
				_targetUrl = "http://line.me/R/msg/text/?"+ _txt+ _br + _url;
				window.open(_targetUrl);
				}else{
					alert("라인 공유하기 기능은 모바일에서만 사용할 수 있습니다.");
				}
			}else if(snsType == "twitter"){
				_targetUrl = "https://twitter.com/intent/tweet?text=" + _txt + "&url=" + _url; 
				window.open(_targetUrl);
			}
			else{
				alert("정의된 SNS가 아닙니다.");
			}



			//퍼가기 카운트 증가
			function shareCntUpdate(pid) {
	            //product_share_cnt
	            $.ajax({
	                url: '/zws/share/shareCntAjax.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB',
	                type: "post",
	                data: "pid=" + pid,
	                dataType: "json",
	                success : function (responseData) {
	                    var data = responseData;
	                    if(!data){
	                        alert("통신 에러");
	                    }
	                    if(data.success){
	                        //alert("공유하기에 추가되었습니다.");
	                        var share_cnt = data.data.share_cnt;
	                        $("#product_share_cnt").text(share_cnt);
	                    }
	                },
	                error: function(request,status,error){
	                    if(request.status==417){
	                        alert("이미 스크랩목록에 존재합니다.");
	                    } else {
	                        alert("공유하기 중에 오류가 발생되었습니다. code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
	                    }
	                }
	            });
	        }
		}
		
		function showHide($val){
			$($val).fadeToggle("fast");
		}

	</script>
