<script>
    function doSimpleEstimateCreate(){
        $("#btn_simple_estimate").prop("disabled", true);
        if(validateSimpleEstimateForm()){
            var form = document.getElementById("simpleEstimateForm");
            var phone_no = $("#simple_tel1").val() + $("#simple_tel2").val() + $("#simple_tel3").val();
            $("#simple_phone_no").val(phone_no);
            form.action = "/zws/estimate/create.do;jsessionid=3DF6B9E9870AE35B86DAAF7CDCD296BB";
            form.method = "post";
            form.submit();
        } else {
            $("#btn_simple_estimate").removeAttr("disabled");
        }
    }

    function validateSimpleEstimateForm(){
        var form = document.getElementById("simpleEstimateForm");
        if($.trim($("#simple_writer").val())==''){
            alert("이름을 입력하세요.");
            $("#simple_writer").focusin();
            return false;
        }

        var simple_writer = $.trim($("#simple_writer").val());

        if(simple_writer.length>30){
            alert("이름을 30글자 이하로 입력하세요.");
            $("#simple_writer").focusin();
            return false;
        }

        if($("#simple_tel2").val()==''){
            alert("전화번호를 입력하세요.");
            $("#simple_tel2").focusin();
            return false;
        }
        if($("#simple_tel3").val()==''){
            alert("전화번호를 입력하세요.");
            $("#simple_tel3").focusin();
            return false;
        }
        if($("#simple_comment").val()==''){
            alert("요청사항을 입력하세요.");
            $("#simple_comment").focusin();
            return false;
        }

        if($.isNumeric( $("#simple_tel2").val())==false){
            alert("전화번호를 숫자로 입력하세요.");
            $("#simple_tel2").focusin();
            return false;
        }

        if($.isNumeric( $("#simple_tel3").val())==false){
            alert("전화번호를 숫자로 입력하세요.");
            $("#simple_tel3").focusin();
            return false;
        }

        var phone_no = $("#simple_tel1").val() + $("#simple_tel2").val() + $("#simple_tel3").val();

        if(phone_no.length>11){
            alert("전화번호를 11자리 이하 숫자로 입력하세요.");
            $("#simple_tel2").focusin();
            return false;
        }

        if(form.agreement_yn.checked==false){
            alert("개인정보 취급방침 동의를 체크하세요.");
            return false;
        }
        
        if(form.agreement_third.checked==false){
            alert("제 3자 제공 동의를 체크하세요.");
            return false;
        }

        //include_static.jsp 파일에 정의되어있음.
        if(grecaptchaUseded && !grecaptchaVerified1){
            alert("로봇이 아닙니다를 선택하세요.(자동글 등록 방지)");
            return false;
        }

        return true;
    }
</script>


