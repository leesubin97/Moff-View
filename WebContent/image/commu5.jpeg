    <!-- / modal btn :: 스크랩북 폼 -->
<script>
    var SCRAP_BUTTON_OBJ;
    var TARGET_PID = 0;
    var SCRAP_COUNT_OBJ;

    function openScrapbookDialog(){
        $("#scrapbookDialog").modal();
        selectScrapbookList();
    }

    function selectScrapbookList(){
        $.ajax({
            url: contextPath + "/zzimBook/getListData.do",
            type: "post",
            data: "",
            dataType: "json",
            success : function (responseData) {
                var data = responseData;
                if(!data){
                    alert("스크랩북 조회 중 오류가 발생되었습니다.");
                }
                if(data.success){
                    var resultList = data.data;

                    if(isNotEmpty(resultList)) {
                        var resultLength = resultList.length;
                        if(resultLength > 0) {
                            $("#scrapbook_list").html("");
                            $("#scrap_book_name").val("");
                            $("#scrap_book_no").val("0");

                            for(var i = 0; i < resultLength; i++) {
                                var templateData = {
                                    "book_no" : resultList[i].book_no,
                                    "book_name" : resultList[i].book_name
                                };
                                drawScrapbookTemplate(templateData);
                            }
                        }
                    }
                }
            },
            error: function(request,status,error){
                alert("스크랩북 조회 중에 오류가 발생되었습니다.");
            }
        });
    }

    function doScrapbookCreate(){
        var params = $("#scrapbookForm").serialize();
        $.ajax({
            url: contextPath + "/zzimBook/create.do",
            type: "post",
            data: params,
            dataType: "json",
            success : function (responseData) {
                var data = responseData;
                if(!data){
                    alert("스크랩북 등록 중 오류가 발생되었습니다.");
                }
                if(data.success){
                    selectScrapbookList();
                }
            },
            error: function(request,status,error){
                alert("스크랩북 등록 중에 오류가 발생되었습니다.");
            }
        });
    }

    function doScrapbookDelete(book_no){
        var params = "cmd=delete&book_no=" + book_no;
        $.ajax({
            url: contextPath + "/zzimBook/delete.do",
            type: "post",
            data: params,
            dataType: "json",
            success : function (responseData) {
                var data = responseData;
                if(!data){
                    alert("스크랩북 삭제 중 오류가 발생되었습니다.");
                }
                if(data.success){
                    selectScrapbookList();
                }
            },
            error: function(request,status,error){
                alert("스크랩북 삭제 중에 오류가 발생되었습니다.");
            }
        });
    }

    function doToggleScrap(_this, pid, scrapCountObjName){
        SCRAP_BUTTON_OBJ = $(_this);
        SCRAP_COUNT_OBJ = $("#" + scrapCountObjName);
        TARGET_PID = pid;
        if (SCRAP_BUTTON_OBJ.hasClass('active')){
            doScrap("delete", pid, 0);
        } else {
            openScrapbookDialog();
        }
    }

    function doScrapCreate(book_no){
        doScrap("create", TARGET_PID, book_no);
    }

    function doScrap(cmd, pid, book_no){
        var me = SCRAP_BUTTON_OBJ;
        var flag = false;
        if (me.hasClass('active')){
            me.removeClass('active');
        } else {
            flag = true;
            me.addClass('active');
        }
        var action_url = contextPath + "/zzim/create.do";

        if(cmd=='create'){
            action_url = contextPath + "/zzim/create.do";
        } else if(cmd=='delete'){
            action_url = contextPath + "/zzim/delete.do";
        }

        $.ajax({
            url: action_url,
            type: "post",
            data: "pid=" + pid + "&type=1" + "&book_no=" + book_no + "&cmd=" + cmd,
            dataType: "json",
            success : function (responseData) {
                var data = responseData;
                if(!data){
                    alert("스크랩 중에 오류가 발생되었습니다.");
                }
                if(data.success){
