<script type="text/javascript">


    var LoginAPI = (function(){
        var logined, success_callback, failure_callback;
        function init(){
            logined = false;
        }
        function setLogined(p_logined){
            logined = p_logined;
        }
        function isLoginedMember(){
            return logined;
        }
        function showLoginLayer(successCallback, failureCallback){
            if(logined) return;

            success_callback = successCallback;
            failure_callback = failureCallback;
            $("#login_window").modal('show');
            setUpLogin();
        }
        function treatLogin(){
            if(validation()) {
                var sendData = $("#layerLoginForm").serialize();

                $.ajax({
                    url: contextPath + '/common/loginAction.do',
                    type: "POST",
                    data: sendData,
                    cache:false,
                    dataType: "json",
                    success : function(responseData) {
                        var data = responseData;
                        if(!data){
                            if(failure_callback != 'undefined' && typeof failure_callback === 'function'){
                                failure_callback(data);
                            } else {
                                alert("통신 에러");
                            }
                        }
                        if(data.success){
                            treatLoginSuccessView();
                            loginCallback(data);

                            //콜백함수가 있으면 실행함
                            if(typeof success_callback === 'function') {
                                success_callback(data);
                            }
                        } else {
                            if(failure_callback != 'undefined' && typeof failure_callback === 'function'){
                                failure_callback(data);
                            } else {
                                alert(data.result_msg);
                            }
                        }

                    },
                    error: function(request,status,error){
                        if(failure_callback != 'undefined' && typeof failure_callback === 'function'){
                            failure_callback(data);
                        } else {
                            alert("로그인 실패하였습니다. code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
                        }
                    }
                });
            }
        }
        function loginCallback(data){
            try {
                if(data.success){
                    logined = true;
                }
            } catch (err) {}
        }
        function setUpLogin(){
            $("#btn_login").on('click', treatLogin);

            //TODO : SNS 로그인 셋업도 여기로 옮기자.

        }
        function treatLoginSuccessView(){
            $(".login-status").each(function(index, obj){
                $(this).show();
            });
            $(".logout-status").each(function(index, obj){
                $(this).hide();
            });
            $('#login_window').modal('hide');
        }
        function validation(){
            var result_flag = true;
            var selector = $("#layerLoginForm");

            if (selector.find("input[name='j_username']").val() == '') {
                alert('id 를 입력하세요.');
                selector.find("input[name='j_username']").focus();
                return false;
            }

            if (selector.find("input[name='j_password']").val() == '') {
                alert('password 를 입력하세요.');
                selector.find("input[name='j_password']").focus();
                return false;
            }
            return result_flag;
        }
        return{
            init:init,
            setLogined:setLogined,
            isLoginedMember:isLoginedMember,
            showLoginLayer:showLoginLayer,
            treatLogin:treatLogin
        }
    })();



